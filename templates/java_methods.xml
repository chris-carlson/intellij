<templateSet group="java_methods">
  <template name="meth" value="$VISIBILITY$ $TYPE$ $NAME$($PARAMETERS$) {&#10;    $END$&#10;}" description="public method declaration" toReformat="false" toShortenFQNames="true">
    <variable name="VISIBILITY" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="&quot;void&quot;" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMETERS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="meths" value="$VISIBILITY$ static $TYPE$ $NAME$($PARAMETERS$) {&#10;    $END$&#10;}" description="public static method declaration" toReformat="false" toShortenFQNames="true">
    <variable name="VISIBILITY" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="&quot;void&quot;" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMETERS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="ret" value="return $EXPRESSION$;" description="return statement" toReformat="false" toShortenFQNames="true">
    <variable name="EXPRESSION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="call" value="$EXPRESSION$($PARAMETERS$);" description="method call" toReformat="false" toShortenFQNames="true">
    <variable name="EXPRESSION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMETERS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="main" value="public static void main(String[] args) {&#10;    $END$&#10;}" description="main method declaration" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="sing" value="private static $CLASS$ instance = new $CLASS$();&#10;&#10;public static $CLASS$ getInstance() {&#10;    return instance;&#10;}" description="singleton instance declaration" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS" expression="className()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="methd" value="default $TYPE$ $NAME$($PARAMETERS$) {&#10;    $END$&#10;}" description="default method declaration" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="&quot;void&quot;" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMETERS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>